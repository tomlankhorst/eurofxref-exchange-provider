{
    "name": "tomlankhorst/eurofxref-exchange-provider",
    "description": "EuroFXRef Exchange Provider for brick/money",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lankhorst",
            "email": "hello@tomlankhorst.nl"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": "^7.1.3",
        "brick/money": "^0.4.0",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
